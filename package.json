    "build:appLoader": "./node_modules/.bin/tsc --skipLibCheck -t 'esnext' -m 'es6' ./AppLoader/src/*.ts --outDir ./build/AppLoader/; ./node_modules/.bin/uglifyjs ./build/AppLoader/*.js -o ./build/AppLoader/MggAppLoader.js;"
  },
