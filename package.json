    "start": "react-scripts start",
    "build": "react-scripts build; yarn build:appLoader",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --record --key 3d9d7df6-b33f-4a65-9979-956bb72e037a",
    "e2e-test": "yarn install && yarn start & echo 'Server started' && echo $! > .server-pid; sleep 5 && yarn cypress:run && yarn e2e-stop",
